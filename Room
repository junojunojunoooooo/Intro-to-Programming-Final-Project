public class Room {
    private int Gold;
    private boolean Monster;
    private int MonsterGen;
    private boolean Blocked;
    private int x;
    private int y;

    public Room() {
        this.Gold = ((int) (Math.random() * 20) + 1);
        this.MonsterGen = ((int) (Math.random() * 1));
        if (MonsterGen == 1)
            this.Monster = true;
        else this.Monster = false;

    }
    public int getGold() {
        return Gold;
    }



}
